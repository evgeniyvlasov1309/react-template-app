{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "baseUrl": "./src"
  },
  "exclude": ["node_modules"],
  "include": ["src", "webpack.config.ts"]
}
